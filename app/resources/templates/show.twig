{% extends "parent.twig" %}

{% block title %}Link{% endblock %}

{% block content %}
    <h2>Short url created! Your new link is:</h2>
        <p class="lead">
            <a href="{{ url }}" target="_blank">{{ url }}</a>
        </p>
        <p class="lead">
            <button type="button" class="btn btn-default btn-lg" onclick="window.location.href='{{ baseUrl }}';">Back</button>
        </p>
{% endblock %}
